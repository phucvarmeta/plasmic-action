#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/create-plasmic-app@0.0.98/node_modules/create-plasmic-app/dist/node_modules:/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/create-plasmic-app@0.0.98/node_modules/create-plasmic-app/node_modules:/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/create-plasmic-app@0.0.98/node_modules:/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/create-plasmic-app@0.0.98/node_modules/create-plasmic-app/dist/node_modules:/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/create-plasmic-app@0.0.98/node_modules/create-plasmic-app/node_modules:/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/create-plasmic-app@0.0.98/node_modules:/Users/phuctran/Work/varmeta/plasmic-action/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-plasmic-app/dist/index.js" "$@"
else
  exec node  "$basedir/../create-plasmic-app/dist/index.js" "$@"
fi
